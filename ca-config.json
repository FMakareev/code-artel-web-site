      "ssr_fetch": true,
      "server_entry": "./src/server/index.tsx",
      "client_entry": "./src/client/index.tsx",
      "output": "dist",
      "endpoint_server": "'http://localhost:1337'",
      "endpoint_client": "'http://localhost:1337'",
      "include": ["home","router"],
      "port": 3021
    },
    "start:ssr:dev":  {
      "NODE_ENV": "development",
      "watch": true,
      "hmr": true,
      "ssr_fetch": true,
      "server_entry": "./src/server/index.tsx",
      "client_entry": "./src/client/index.tsx",
      "output": "dist",
      "endpoint_server": "'http://localhost:1337'",
      "endpoint_client": "'http://localhost:1337'",
      "include": ["home","router"],
      "port": 3021
    },
    "build:ssr:prod": {
      "NODE_ENV": "production",
      "watch": false,
      "hmr": false,
      "ssr_fetch": true,
      "server_entry": "./src/server/index.tsx",
      "client_entry": "./src/client/index.tsx",
      "output": "dist",
      "endpoint_server": "'http://localhost:1337'",
      "endpoint_client": "''",
      "include": ["home","router"],
      "port": 3021
    },
    "build:ssr:dev": {
      "NODE_ENV": "development",
      "watch": false,
      "hmr": false,
      "ssr_fetch": true,
      "server_entry": "./src/server/index.tsx",
      "client_entry": "./src/client/index.tsx",
      "output": "dist",
      "endpoint_server": "'http://localhost:1337'",
      "endpoint_client": "''",
      "include": ["home","router"],
      "port": 3021
    },
    "start:ssr:server": {
      "port": 3021,
      "public": "dist"
    }
  },
  "csr": {
    "start:csr:prod":{
      "NODE_ENV": "production",
      "hmr": true,
      "watch": true,
      "client_entry": "./src/client/index.tsx",
